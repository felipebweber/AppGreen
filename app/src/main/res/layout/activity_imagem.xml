<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_video"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="com.example.felipe.appgreen.Activity.VideoActivity">


    <TextView
        android:id="@+id/textViewKi"
        android:layout_alignBaseline="@+id/btMenosKi"
        android:layout_alignBottom="@+id/btMenosKi"
        android:layout_toEndOf="@+id/btMenosKi"
        android:layout_height="25dp"
        android:layout_width="40dp"
        android:textAlignment="center" />

    <ImageView
        android:layout_width="match_parent"
        android:id="@+id/imageView"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_height="400dp" />

    <Button
        android:text="-"
        android:id="@+id/btMenosKi"
        android:layout_width="35dp"
        android:layout_height="35dp"

        android:layout_below="@+id/textView2"
        android:layout_alignParentStart="true"
        android:textSize="12sp" />

    <Button
        android:text="+"
        android:id="@+id/btMaisKi"
        android:layout_width="35dp"
        android:layout_height="35dp"

        android:layout_alignBottom="@+id/btMenosKi"
        android:layout_toEndOf="@+id/textViewKi"
        android:textSize="12sp" />

    <TextView
        android:text="K"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView2"

        android:layout_below="@+id/imageView"
        android:layout_toEndOf="@+id/btMenosK"
        android:layout_marginStart="15dp" />

    <TextView
        android:text="T"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView6"
        android:layout_above="@+id/textViewTi"
        android:layout_alignStart="@+id/textView2" />

    <TextView
        android:layout_width="40dp"
        android:layout_height="25dp"
        android:id="@+id/textViewTi"
        android:textAlignment="center"
        android:layout_marginBottom="9dp"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/btMenosTi"
        android:layout_alignTop="@+id/textViewCi" />

    <Button
        android:text="-"
        android:id="@+id/btMenosTi"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_alignBottom="@+id/textViewTi"
        android:layout_alignParentStart="true"
        android:textSize="12sp" />

    <Button
        android:text="+"
        android:id="@+id/btMaisTi"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_alignBottom="@+id/textViewTi"
        android:layout_toEndOf="@+id/textViewTi"
        android:textSize="12sp" />

    <Button
        android:text="+"
        android:id="@+id/btMaisEi"
        android:layout_width="35dp"
        android:layout_height="35dp"

        android:textSize="12sp"
        android:layout_alignBottom="@+id/textViewKi"
        android:layout_alignParentEnd="true" />

    <TextView
        android:id="@+id/textViewDi"
        android:layout_height="25dp"
        android:layout_width="40dp"
        android:textAlignment="center"
        android:layout_alignBaseline="@+id/btMaisDi"
        android:layout_alignBottom="@+id/btMaisDi"
        android:layout_toStartOf="@+id/btMaisDi" />

    <Button
        android:text="-"
        android:id="@+id/btMenosEi"
        android:layout_width="35dp"
        android:layout_height="35dp"

        android:textSize="12sp"
        android:layout_alignBaseline="@+id/imageView"
        android:layout_alignBottom="@+id/btMaisKi"
        android:layout_toStartOf="@+id/textViewEi" />

    <Button
        android:text="+"
        android:id="@+id/btMaisDi"
        android:layout_width="35dp"
        android:layout_height="35dp"

        android:textSize="12sp"
        android:layout_alignBaseline="@+id/btMaisTi"
        android:layout_alignBottom="@+id/btMaisTi"
        android:layout_alignParentEnd="true" />

    <TextView
        android:text="E"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView3"

        android:layout_marginEnd="9dp"
        android:layout_below="@+id/imageView"
        android:layout_alignStart="@+id/textView5" />

    <TextView
        android:id="@+id/textViewEi"
        android:layout_height="25dp"
        android:layout_width="40dp"
        android:textAlignment="center"
        android:layout_alignBaseline="@+id/btMaisEi"
        android:layout_alignBottom="@+id/btMaisEi"
        android:layout_toStartOf="@+id/btMaisEi" />

    <Button
        android:text="-"
        android:id="@+id/btMenosDi"
        android:layout_width="35dp"
        android:layout_height="35dp"

        android:textSize="12sp"
        android:layout_alignBottom="@+id/textViewDi"
        android:layout_toStartOf="@+id/textViewDi" />

    <TextView
        android:text="D"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView5"

        android:layout_marginEnd="13dp"
        android:layout_above="@+id/textViewDi"
        android:layout_toStartOf="@+id/btMaisDi" />

    <Button
        android:text="RST"
        android:layout_marginStart="9dp"
        android:id="@+id/btResetKeTi"
        android:textSize="8sp"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignTop="@+id/btMaisKi"
        android:layout_toEndOf="@+id/btMaisKi" />

    <Button
        android:text="RST"
        android:id="@+id/btResetEeDi"
        android:textSize="8sp"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginEnd="7dp"
        android:layout_alignTop="@+id/btMaisEi"
        android:layout_toStartOf="@+id/btMenosEi" />

    <TextView
        android:layout_width="40dp"
        android:layout_height="25dp"
        android:id="@+id/textViewCi"
        android:textAlignment="center"
        android:layout_marginBottom="11dp"
        android:layout_alignParentBottom="true"
        android:layout_toStartOf="@+id/btMaisCi" />

    <TextView
        android:text="C"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView8"
        android:layout_above="@+id/textViewDi"
        android:layout_toStartOf="@+id/btResetEeDi"
        android:layout_marginEnd="12dp" />

    <Button
        android:text="-"
        android:id="@+id/btMenosCi"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:textSize="12sp"
        android:layout_below="@+id/textView8"
        android:layout_alignEnd="@+id/btResetKeTi" />

    <Button
        android:text="+"
        android:id="@+id/btMaisCi"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:textSize="12sp"
        android:layout_below="@+id/textView8"
        android:layout_alignEnd="@+id/btResetEeDi" />

</RelativeLayout>
